{
  "codeLens.enable": true,
  "jedi.enable": true,
  "jedi.startupMessage": true,
  "jedi.markupKindPreferred": "plaintext",
  "jedi.trace.server": true,
  "jedi.jediSettings.autoImportModules": [],
  "jedi.executable.args": [],
  "jedi.completion.disableSnippets": false,
  "jedi.diagnostics.enable": true,
  "jedi.diagnostics.didOpen": true,
  "jedi.diagnostics.didChange": true,
  "jedi.diagnostics.didSave": true,
  "languageserver": {
    "elixirLS": {
      "command": "~/.elixir/elixir_ls/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    },
    "go": {
      "command": "gopls",
      "args": ["serve", "-debug", "0.0.0.0:8484", "-rpc.trace"],
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "solargraph.commandPath": "~/.asdf/shims/solargraph",
  "solargraph.promptDownload": true,
  "solargraph.diagnostics": true,
  "solargraph.completion": true,
  "solargraph.formatting": true,
  "solargraph.definitions": true,
  "solargraph.references": true,
  "solargraph.folding": true,
  "solargraph.userBundler": false
}
